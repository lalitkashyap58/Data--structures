class Solution {
public:
    string largestOddNumber(string num) {
        int size=num.size();
        string st="";
        if(num[size-1]%2!=0)
        return num;

        for(int i=size-1;i>=0;i--)
        {
            if(int(num[i]%2)!=0)
            {cout<<"run"<<endl;
            return num.substr(0,i+1);
            }
        }
       
        return st;
        
    }
};
